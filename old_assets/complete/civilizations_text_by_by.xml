	<Replace Language="by_BY" Tag="LOC_TRAIT_CIVILIZATION_DYNASTIC_CYCLE_NAME">
      <Text>Дынастычныя цыклы</Text>
    </Replace>				
	<Replace Language="by_BY" Tag="LOC_TRAIT_CIVILIZATION_DYNASTIC_CYCLE_DESCRIPTION">
